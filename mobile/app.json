{
  "expo": {
    "name": "TabaFit",
    "slug": "Abcountable",
    "version": "0.2.4",
    "orientation": "portrait",
    "icon": "./assets/tabafit-ios-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/wide_tabafit_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f1317"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mattkuda.TabaFit",
      "icon": "./assets/tabafit-ios-icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "a99c6905-3b89-4210-b5b7-ba66ed2975ee"
      }
    }
  }
}
